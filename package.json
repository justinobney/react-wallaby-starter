{
  "name": "ReactStarter",
  "version": "0.0.1",
  "scripts": {
    "start": "react-scripts start",
    "now-start": "serve --single ./build",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor=jest-junit-reporter",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-links": "^3.2.12",
    "@storybook/react": "^3.2.12",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "cross-env": "^5.0.5",
    "jest-junit-reporter": "^1.1.0",
    "jest-styled-components": "^4.5.0",
    "react-scripts": "^1.0.13",
    "react-test-renderer": "^16.0.0",
    "serve": "^6.2.0"
  },
  "dependencies": {
    "actionware": "^8.1.0",
    "axios": "^0.16.2",
    "core-js": "^2.5.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tagsinput": "^3.17.0",
    "recharts": "^1.0.0-apha.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0",
    "simple-pagination": "^1.0.0",
    "slug": "^0.9.1",
    "styled-components": "^2.1.2",
    "toastr": "^2.1.2",
    "validate.js": "^0.11.1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "jasmine": true
    },
    "rules": {
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "error",
        2
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "no-use-before-define": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "parser": "babylon",
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5"
  }
}
