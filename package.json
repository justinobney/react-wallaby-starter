{
  "name": "justinobney.react-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "react-app-rewired start",
    "now-start": "serve --single ./build",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:ci": "cross-env CI=true react-app-rewired test --env=jsdom --testResultsProcessor=jest-junit-reporter",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "3.4.6",
    "@storybook/addon-info": "3.4.6",
    "@storybook/addon-knobs": "^3.4.6",
    "@storybook/addon-links": "3.4.6",
    "@storybook/react": "3.4.6",
    "babel-plugin-emotion": "9.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-latest": "6.24.1",
    "cross-env": "5.1.6",
    "jest-emotion": "9.1.3",
    "jest-junit-reporter": "1.1.0",
    "prop-types": "15.6.1",
    "react-app-rewire-emotion": "4.0.0",
    "react-app-rewire-hot-loader": "1.0.1",
    "react-app-rewired": "1.5.2",
    "react-hot-loader": "4.3.0",
    "react-scripts": "1.1.4",
    "react-test-renderer": "16.4.0",
    "react-testing-library": "^3.1.3",
    "serve": "8.1.0"
  },
  "dependencies": {
    "actionware": "8.2.3",
    "axios": "0.18.0",
    "emotion": "9.1.3",
    "lodash": "4.17.10",
    "polished": "1.9.2",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-emotion": "9.1.3",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "segmentize": "0.4.1",
    "semantic-ui-css": "2.3.1",
    "semantic-ui-react": "0.81.1",
    "toastr": "2.1.4",
    "validate.js": "0.12.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "jasmine": true
    },
    "rules": {
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "error",
        2
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "no-use-before-define": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "parser": "babylon",
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  }
}
